<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pl.edu.pja.s13868.miniproject1" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".EduApplication">

        <activity
            android:name=".ui.activity.MyMainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:parentActivityName=".ui.activity.MyMainActivity"
            android:name=".ui.activity.MyOptionsActivity"
            android:label="@string/title_activity_options">
        </activity>
        <activity
            android:parentActivityName=".ui.activity.MyMainActivity"
            android:name=".ui.activity.MyListActivity"
            android:label="@string/title_activity_product_list" >
        </activity>
        <activity
            android:parentActivityName=".ui.activity.MyListActivity"
            android:name=".ui.activity.EditActivity"
            android:label="@string/title_activity_edit">
        </activity>

    </application>

</manifest>
